{
    "name": "devhun/vfax-api-client",
    "description": "PHP API client for VFax (SEJONG Telecom Service)",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/devhun/vfax-api-client",
    "authors": [
        {
            "name": "devhun",
            "email": "i@huns.kr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "guzzlehttp/guzzle": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "DevHun\\VFax\\": "src"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "To use GuzzleHttpAdapter, require guzzlehttp/guzzle:^6.0"
    }
}
